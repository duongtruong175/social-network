<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white">

    <View
        android:id="@+id/skeleton_1"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_marginTop="12dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/bg_round_4"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <View
        android:id="@+id/skeleton_3"
        android:layout_width="80dp"
        android:layout_height="136dp"
        android:background="@drawable/bg_round_8"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toBottomOf="@id/skeleton_1"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toStartOf="parent"/>

    <View
        android:id="@+id/skeleton_4"
        android:layout_width="80dp"
        android:layout_height="136dp"
        android:background="@drawable/bg_round_8"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toTopOf="@id/skeleton_3"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@id/skeleton_3"/>

    <View
        android:id="@+id/skeleton_5"
        android:layout_width="80dp"
        android:layout_height="136dp"
        android:background="@drawable/bg_round_8"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toTopOf="@id/skeleton_4"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@id/skeleton_4"/>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/skeleton_6"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:src="@color/color_shimmer"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/skeleton_3"
        android:layout_marginTop="30dp"/>

    <View
        android:id="@+id/skeleton_7"
        android:layout_width="200dp"
        android:layout_height="16dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/bg_round_4"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toTopOf="@+id/skeleton_6"
        app:layout_constraintStart_toEndOf="@+id/skeleton_6"/>

    <View
        android:id="@+id/skeleton_8"
        android:layout_width="160dp"
        android:layout_height="16dp"
        android:background="@drawable/bg_round_4"
        android:layout_marginTop="2dp"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toBottomOf="@+id/skeleton_7"
        app:layout_constraintStart_toStartOf="@+id/skeleton_7"/>

    <View
        android:id="@+id/skeleton_9"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="@drawable/bg_round_4"
        android:layout_marginTop="8dp"
        android:layout_margin="8dp"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toBottomOf="@+id/skeleton_6"/>

    <View
        android:id="@+id/skeleton_10"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:background="@drawable/bg_round_4"
        android:layout_marginTop="8dp"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toBottomOf="@+id/skeleton_9"/>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/skeleton_11"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:src="@color/color_shimmer"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/skeleton_10"
        android:layout_marginTop="30dp"/>

    <View
        android:id="@+id/skeleton_12"
        android:layout_width="200dp"
        android:layout_height="16dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/bg_round_4"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toTopOf="@+id/skeleton_11"
        app:layout_constraintStart_toEndOf="@+id/skeleton_11"/>

    <View
        android:id="@+id/skeleton_13"
        android:layout_width="160dp"
        android:layout_height="16dp"
        android:background="@drawable/bg_round_4"
        android:layout_marginTop="2dp"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toBottomOf="@+id/skeleton_12"
        app:layout_constraintStart_toStartOf="@+id/skeleton_12"/>

    <View
        android:id="@+id/skeleton_14"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="@drawable/bg_round_4"
        android:layout_marginTop="8dp"
        android:layout_margin="8dp"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toBottomOf="@+id/skeleton_11"/>

    <View
        android:id="@+id/skeleton_15"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:background="@drawable/bg_round_4"
        android:layout_marginTop="8dp"
        android:backgroundTint="@color/color_shimmer"
        app:layout_constraintTop_toBottomOf="@+id/skeleton_14"/>

</androidx.constraintlayout.widget.ConstraintLayout>